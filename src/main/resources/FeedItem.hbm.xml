<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
	<class name="xyz.maksimenko.feedsreader.feedobject.FeedItem" table="feeditems">
		<id column="itemId" name="itemId" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		<property column="title" name="title" type="java.lang.String"/>
		<property column="guid" name="guid" type="java.lang.String"/>
		<property column="link" name="link" type="java.lang.String"/>
		<property column="pubdate" name="pubdate" type="java.lang.Long"/>
		<property column="author" name="author" type="java.lang.String"/>
		<property column="readstatus" name="readstatus" type="java.lang.Byte"/>
		<property column="description" name="description" type="java.lang.String"/>
	</class>
</hibernate-mapping>